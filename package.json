{
  "name": "soranix",
  "version": "1.0.0",
  "description": "Soranix is a AI powered operating system for finance ",
  "main": "index.js",
  "workspaces": [
    "apps/*",
    "packages/*",
    "services/*",
    "tooling/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:env": "node scripts/start.js",
    "start:client": "npm run dev --workspace=@soranix/client",
    "start:auth": "npm run dev --workspace=@soranix/auth",
    "prepare": "husky",
    "commitlint": " commitlint --edit -c  ",
    "commit": "commit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Soranix-inc/soranix.git"
  },
  "keywords": [
    "Finance",
    "AI",
    "Budget"
  ],
  "author": "Soranix",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Soranix-inc/soranix/issues"
  },
  "packageManager": "npm@11.4.1",
  "homepage": "https://github.com/Soranix-inc/soranix#readme",
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@commitlint/cz-commitlint": "^19.8.1",
    "husky": "^9.1.7",
    "turbo": "^2.5.4"
  },
  "dependencies": {
    "@commitlint/prompt-cli": "^19.8.1"
  }
}
